{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_connections_active",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "yellow", "value": 500 },
          { "color": "red", "value": 1000 }
        ]}}
      }
    },
    {
      "title": "Active Broadcasters",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_broadcasters_active",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Active Rooms",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_rooms_active",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Cache Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_cache_size",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Backpressure Drops",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_backpressure_drops",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "orange", "value": 1 },
          { "color": "red", "value": 100 }
        ]}}
      }
    },
    {
      "title": "Backpressure Disconnects",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "targets": [
        {
          "expr": "atmosphere_backpressure_disconnects",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "red", "value": 1 }
        ]}}
      }
    },
    {
      "title": "Connections / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "rate(atmosphere_connections_total_total[5m])",
          "legendFormat": "connects/s",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "rate(atmosphere_connections_disconnects_total[5m])",
          "legendFormat": "disconnects/s",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Messages / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "rate(atmosphere_messages_broadcast_total[5m])",
          "legendFormat": "broadcast/s",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "rate(atmosphere_messages_delivered_total[5m])",
          "legendFormat": "delivered/s",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Broadcast Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(atmosphere_broadcast_timer_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "histogram_quantile(0.95, rate(atmosphere_broadcast_timer_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "histogram_quantile(0.99, rate(atmosphere_broadcast_timer_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Room Members",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "atmosphere_rooms_members",
          "legendFormat": "{{ room }}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Cache Hit / Miss Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "atmosphere_cache_hits",
          "legendFormat": "hits",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "atmosphere_cache_misses",
          "legendFormat": "misses",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "title": "Cache Evictions",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "atmosphere_cache_evictions",
          "legendFormat": "evictions",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["atmosphere", "realtime", "websocket"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Atmosphere 4.0 â€” Real-time Monitoring",
  "uid": "atmosphere-4-realtime",
  "version": 1
}
