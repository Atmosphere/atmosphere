<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<module rename-to="gwtprojsonserializer">
    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="com.google.gwt.user.User"/>
    <generate-with class="com.kfuntak.gwt.json.serialization.SerializationGenerator">
		<when-type-assignable class="com.kfuntak.gwt.json.serialization.client.Serializer"/>
	</generate-with>
	<entry-point class="com.kfuntak.gwt.json.serialization.client.GWTProJsonSerializer"/>
</module>
